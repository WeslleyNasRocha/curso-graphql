{
  "name": "curso_graphql",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.0",
    "@types/express-graphql": "0.8.0",
    "@types/graphql": "14.2.1",
    "@types/node": "12.0.10",
    "@types/sequelize": "4.0.76",
    "gulp": "4.0.2",
    "gulp-clean-dir": "1.0.2",
    "gulp-typescript": "5.0.1",
    "nodemon": "1.19.1",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon",
    "start:watch": "ts-node --files ./src/index.ts",
    "gulp:dev": "gulp",
    "gulp": "gulp build"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:watch",
    "ext": "ts"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "express": "4.17.1",
    "express-graphql": "0.8.0",
    "graphql": "14.3.1",
    "graphql-tools": "4.0.5",
    "mysql2": "1.6.5",
    "sequelize": "5.15.1"
  }
}